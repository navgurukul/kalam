{
  "name": "react-boiler-plate-code",
  "version": "1.0.0",
  "description": "react-boiler-plate-code",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf dist",
    "build": "mkdir dist; cp public/index.html dist/index.html && cp public/favicon.ico dist/favicon.ico && cp public/index.html dist/404.html && NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config webpack.prod.js",
    "dev": "webpack serve --config webpack.dev.js",
    "predeploy": "npm run build && cd src/config && git pull origin master && cd ../..",
    "deploy": "gh-pages -d dist",
    "lint": "eslint src --fix",
    "test": "true"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "cname": "kalam.navgurukul.org",
    "prep": [
      "predeploy"
    ],
    "post": [
      "clean"
    ],
    "noprompt": true
  },
  "homepage": "https://navgurukul.github.io/kalam",
  "author": "NavGurukul",
